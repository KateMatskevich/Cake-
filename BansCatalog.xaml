                    </Grid>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Margin="131,45,18,179" BorderBrush="{x:Null}" Background="{x:Null}" RenderTransformOrigin="1.814,0.582" Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="1" Grid.RowSpan="2">

            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Ellipse Stroke="{x:Null}"
                            StrokeThickness="2">
                            <Ellipse.Fill>
                                <ImageBrush ImageSource="pack://application:,,,/img/v27.jpg" Stretch="UniformToFill"/>
                            </Ellipse.Fill>

                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center"
                            VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Margin="8,13,15,1" BorderBrush="{x:Null}" Background="{x:Null}" RenderTransformOrigin="1.814,0.582" Grid.Column="4" Grid.Row="2">

            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Ellipse Stroke="{x:Null}"
                            StrokeThickness="2">
                            <Ellipse.Fill>
                                <ImageBrush ImageSource="pack://application:,,,/img/v22.jpg" Stretch="UniformToFill"/>
                            </Ellipse.Fill>

                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center"
                            VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button Margin="135,195,14,80" BorderBrush="{x:Null}" Background="{x:Null}" RenderTransformOrigin="1.814,0.582" Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="2" Grid.RowSpan="3">

            <Button.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Ellipse Stroke="{x:Null}"
                            StrokeThickness="2">
                            <Ellipse.Fill>
                                <ImageBrush ImageSource="pack://application:,,,/img/v14.png" Stretch="UniformToFill"/>
                            </Ellipse.Fill>

                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center"
                            VerticalAlignment="Center"/>
                    </Grid>
